<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_Skip_Contact_Create_to_match_Lead</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL(Contact_Matching_No_Match_Behavior__c, &apos;Create Contact&apos;) &amp;&amp;
ISPICKVAL(Lead_matching_rules__c, &apos;Use Salesforce Duplicate Rule&apos;)</errorConditionFormula>
    <errorMessage>When Lead matching is enabled, Contact Matching No Match Behavior value must be &quot;Skip Creating Contact&quot;</errorMessage>
</ValidationRule>
